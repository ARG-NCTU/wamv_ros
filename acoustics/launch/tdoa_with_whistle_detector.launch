<launch>
    <arg name="yaml_file" default="tdoa_with_whistle_detector_params"/>

    <node name="tdoa" pkg="acoustics" type="tdoa_with_whistle_detector.py" output="screen" clear_params="true" required="true">
        <rosparam command="load" file="$(find acoustics)/config/$(arg yaml_file).yaml" />
    </node>
</launch>
